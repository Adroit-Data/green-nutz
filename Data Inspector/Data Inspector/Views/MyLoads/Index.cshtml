@model IEnumerable<Data_Inspector.Models.LoadedFile>

@{
    ViewBag.Title = "Index";
}
<div class="jumbotron"><h2>My Loads</h2></div>


<a href="~/Load" class="btn btn-primary">Create New</a>
    @*@Html.ActionLink("Create New", "Create")*@


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FileName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FileType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FileImportDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model ) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FileName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FileType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FileImportDate)
        </td>
        <td>            
            @Html.ActionLink("View", "View", new { id=item.LoadedFileID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.LoadedFileID })
        </td>
    </tr>
}

</table>
